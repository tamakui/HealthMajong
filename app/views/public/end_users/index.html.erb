<div class="container px-5 px-sm-0">
  <div class='row'>
    <div class="col-sm-8 px-sm-0 offset-sm-1">
      <h1>会員一覧</h1>
    </div>
    <div class="container-card">
      <% @end_users.each do |end_user| %>
        <div class="card" style="width: 18rem;">
          <%= image_tag end_user.get_profile_image(100, 100), size:'100x100' %>
          <div class="card-body">
            <h5 class="card-login_name">ログイン名</h5>
              <%= link_to end_user.login_name, end_user_path(end_user) %>
            <p class="card-introduction">自己紹介</p>
              <%= end_user.introduction %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>

